{
  "name": "static-landing",
  "version": "2.5.1",
  "description": "Astro static site generator landing page template for Seed & Source stack",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.astro",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "astro",
    "static",
    "landing",
    "ssg"
  ],
  "author": "Seed & Source",
  "license": "MIT",
  "dependencies": {
    "astro": "^5.0.0",
    "@astrojs/tailwind": "^6.0.0",
    "@astrojs/sitemap": "^3.2.1",
    "tailwindcss": "^3.4.0",
    "astro-compress": "^2.3.5"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.0",
    "astro-eslint-parser": "^0.16.0",
    "@typescript-eslint/parser": "^6.19.0",
    "typescript": "^5.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "vitest": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "@playwright/test": "^1.40.0"
  }
}
